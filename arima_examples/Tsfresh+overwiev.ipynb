   "cell_type": "code",
   "execution_count": 191,
   "metadata": {},
   "outputs": [],
   "source": [
    "#select train data \n",
    "price = pd.read_csv(\"D:/UDSC/Time series/player_price.csv\")\n",
    "price['Date'] = pd.to_datetime(price['timestamp'], unit='ms')\n",
    "price.drop([\"timestamp\", \"ps_price\", \"player_name\"], axis=1, inplace=True)\n",
    "\n",
    "\n",
    "data = price[(price[\"Date\"] <= price[\"Date\"].max() - timedelta(days=7)) & \n",
    "                   (price[\"Date\"] > price[\"Date\"].max() - timedelta(days=37))]\n",
    "data = data[(data['player_id'] == 1) | (data['player_id'] == 4) | (data['player_id'] == 2)]\n"
   ]
